{
    "name": "techhimalayasoft/himalayasms",
    "description": "Laravel Package to send SMS via Himalaya SMS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tech Himalaya",
            "email": "info@techhimalaya.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.0|^8.0",
